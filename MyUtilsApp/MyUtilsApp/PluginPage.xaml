             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyUtilsApp.PluginPage">

    <StackLayout VerticalOptions="CenterAndExpand" HorizontalOptions="Center" Spacing="20">
        <Label x:Name="LblBattery"/>
        <Label x:Name="LblGPSStatus"/>
        <Label x:Name="LblGPSLatitude"/>
        <Label x:Name="LblGPSLongitude"/>
        <Label x:Name="LblMessage"/>
        <Label x:Name="LblHeat" />
    </StackLayout>

</ContentPage>